-
  let data = getLink()
  let users = getUsers()
doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Document
    link(href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600&display=swap" rel="stylesheet")
    link(rel="stylesheet" href="./src/css/style.min.css")
    //- link(rel="stylesheet" href="./src/scss/style.scss")
  body.is_scroll(onClick="hideMenu(event)") 
    .container
      aside
        header.header_aside 
          a.logo 
            svg(width="37" height="37")
              use(xlink:href="./src/svg/sprite.svg#logo")
            span Dashboard#[sub.sub_logo v.01]
        nav 
          ul.nav
            each link in data 
              li
                a.menu_list(href="#")
                  svg(class=link.svgName width=link.size height=link.size)
                    use(xlink:href=`./src/svg/sprite.svg#` + link.svgName)
                  p.nav_link #{link.name}
                  if(link.nameButton)
                    button(type="button")
                      svg.stroke(width=link.sizeButton height=link.sizeButton)
                        use(xlink:href=`./src/svg/sprite.svg#` + link.nameButton)
        .manager 
          img(src="./src/images/manager.png", alt="manager")
          .persone 
            h3.manager_name Evano
            p.manager_major Project Manager
      main
        header.main_header 
          h1.main_tytle 
            img(src="./src/images/hello.png", alt="hello")
            button.button_menu(type="button" onclick="showMenu()") Menu
        section.page 
          h2.main_title.visually_hidden main
          .search_container 
            header.page_header 
              .page_tytle 
                h2 All Customers
                p Active Members
              form.search_form(action="#")
                input.search_input(type="text" placeholder="Search")
                button.search_button(type="button")
                  svg(width="24" height="24")
                    use(xlink:href="./src/svg/sprite.svg#search") 
          .page_container 
            table.table 
              thead 
                tr.page_caption
                  td Customer Name
                  td Company
                  td Phone Number
                  td Email
                  td Country
                  td.state Status
              tbody
                each user in users
                  tr.page_tbody 
                    td= user.customerName
                    td= user.company
                    td= user.phoneNumber
                    td= user.email
                    td= user.country
                    if (user.unitedStates) 
                      td.state #[p.is-active Active]
                    else if(!user.unitedStates)
                      td.state #[p.inactive Inactive]
          .pagination_container 
            p.counter_page Showing data 1 to #{users.length} of 256K entries
            .pagination
              button.pages(type="button")
                svg.prev
                  use(xlink:href="./src/svg/sprite.svg#prev") 
              button.pages.active(type="button") 1
              button.pages(type="button") 2
              button.pages(type="button") 3
              button.pages(type="button") 4
              button.void(type="button") ...
              button.pages(type="button") 40
              button.pages(type="button")
                svg.next
                  use(xlink:href="./src/svg/sprite.svg#next")  
    .backdrop
  script(src="src/index.js")  
                  



